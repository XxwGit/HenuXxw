<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fitsSystemWindows="true"
    android:background="#00000000">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/fragement_fifith"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context="com.henu.zhihu.fragment.FifthFragement"
        android:weightSum="1"
        android:background="#F0F4F5">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="10dp"/>

        <LinearLayout
            android:id="@+id/load"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="vertical"
            android:weightSum="1"
            android:background="#ffffff"
            >
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="#ffffff"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_gravity="center_vertical"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="90dp"
                    android:layout_height="29dp"
                    android:src="@drawable/touxiang"
                    android:layout_weight="0.01"
                    android:layout_gravity="left"
                    android:id="@+id/imageView2" />

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/myAttention"
            android:orientation="horizontal"
            android:weightSum="1"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/attention"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/myAttention1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的关注"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:weightSum="1"
            android:id="@+id/myCollection"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/collection"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/myCollection1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的收藏"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:weightSum="1"
            android:id="@+id/myPaper"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/paper"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/myPaper1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的草稿"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:orientation="horizontal"
            android:id="@+id/recentView"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/recent"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/recentView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="最近浏览"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:orientation="horizontal"
            android:id="@+id/mybook"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/book"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/mybook1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的书架"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:orientation="horizontal"
            android:id="@+id/mylive"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/live"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/mylive1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的Live"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:orientation="horizontal"
            android:id="@+id/myzhi"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/zhi"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/myzhi1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的值乎"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

        <View
            android:layout_width="336dp"
            android:layout_height="10dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:id="@+id/night"
            android:orientation="horizontal"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/night"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>
            <TextView
                android:id="@+id/night1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="夜间模式"
                android:textSize="18sp"
                android:layout_gravity="center"/>
            <Switch
                android:id="@+id/switch1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="160dp"
                android:layout_gravity="center"/>
        </LinearLayout>
        <View
            android:layout_width="336dp"
            android:layout_height="1dp"
            android:layout_gravity="right"
            android:background="#00000000"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:weightSum="1"
            android:id="@+id/set"
            android:orientation="horizontal"
            android:background="#ffffff">
            <ImageView
                android:layout_width="46dp"
                android:layout_height="29dp"
                android:src="@drawable/set"
                android:layout_weight="0.01"
                android:layout_gravity="center"/>

            <TextView
                android:id="@+id/set1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设置"
                android:textSize="18sp"
                android:layout_gravity="center"/>
        </LinearLayout>

    </LinearLayout>
</ScrollView>
